{
    "compilerOptions": {
      "lib": ["es2020"],
      "target": "es2020",
      "strict": false,
      "module": "CommonJS",    
      "esModuleInterop": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "strictPropertyInitialization": false,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "outDir": "dist"
    },
    "include": ["src/**/*"],
    "exclude": ["**/*.spec.ts"]
  }